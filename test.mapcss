*
{
  z-index: 0;
  adfasdf: adasdf;
  fill-color: none;
}
point[place] {
  text: eval(concat('== ', tag(name), ' =='));
  z-index: 10;

  font-family: "DejaVu Sans Book";
  font-size: 12;
  text-color: #000000;
  text-halo-radius: 1;
  text-halo-fill: #ffffff;
}
area {
  width: 1;
  color: #A0A0A0;
}
way|z11-::casing
{
  z-index: -20;
  object-z-index: -1;
}
way|z11-14[highway=primary]
{
  width: 2;
  color: #ff0000;
}
way|z11-14[highway=primary]::casing
{
  width: 4;
  color: #A0A0A0;
}

way|z15-[highway=primary]
{
  width: 4;
  color: #ff0000;
}
way|z15-[highway=primary]::casing
{
  width: 7;
  color: #A0A0A0;
}
way|z11-[bridge]::casing
{
  color: #000000;
}

way[highway=residential],
way[highway=unclassified] {
  set .minor_road;
  set foo=bar;
  set test=eval(tag(name));
  set foo1=eval(2*5+1-3);
  foo2: eval("2+-3");
  unset name;
}
way[foo=bar] {
  color: #00ff00;
}
way.minor_road {
  width: 2;
  z-index: -1;
}
